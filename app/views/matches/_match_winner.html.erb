<div class="row">
		<div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
			<h1> <%= match.title %></h1>
			<h2> Ganador: <%= users.find(match.winner).email %></h2>	
			<h2> Puntuacion: <%= match.score %></h2>
		</div>
</div>
<div class="row">
		<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 text-center">
			<%= link_to "Nuevo Juego", new_match_path, class: "btn btn-primary"  %>
			<%= link_to "Buscar Juego", search_match_path, class: "btn btn-primary"  %>
		</div>
</div>